{
  "name": "kni-scss",
  "version": "1.0.0",
  "description": "kni css",
  "main": "scss",
  "engines": {
    "node": "^16.15.1",
    "npm": "^8.5.5"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "deploy": "npx stylelint \"**/*.{css,sass,scss}\" && npm run sass-prod",
    "prepare": "husky install",
    "prettier": "npx prettier --write .",
    "sass-dev": "npm i && sass --watch --update --style=expanded test:test",
    "sass-prod": "sass --style=compressed test:test",
    "stylelint": "npx stylelint \"**/*.{css,sass,scss}\" --fix"
  },
  "author": "KNI",
  "license": "ISC",
  "dependencies": {
    "husky": "^8.0.1",
    "sass": "^1.54.9",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0"
  },
  "devDependencies": {
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  }
}
