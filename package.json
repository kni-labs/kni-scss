{
  "name": "kni-scss",
  "version": "3.0.0",
  "description": "kni css",
  "main": "scss",
  "engines": {
    "node": ">=16 <19",
    "npm": ">=8 <10"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run gulp-build",
    "deploy": "npm run eslint && npm run stylelint && npm run build",
    "eslint": "npx eslint \"./**/*.{js,mjs,jsx,ts,tsx,mdx}\" --max-warnings=0 --fix",
    "gulp": "gulp",
    "gulp-build": "gulp build-sass",
    "prepare": "husky install",
    "prettier": "npx prettier --write .",
    "stylelint": "npx stylelint \"**/*.{css,sass,scss}\" --fix"
  },
  "author": "KNI",
  "license": "ISC",
  "dependencies": {
    "eslint": "^8.23.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-purge-sourcemaps": "^1.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.1",
    "sass": "^1.54.9",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0"
  },
  "devDependencies": {
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  }
}
